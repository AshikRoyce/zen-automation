{
  "name": "zen-automation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "serve": "nodemon ./bin/www",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^26.0.1",
    "morgan": "~1.9.1",
    "puppeteer": "^4.0.0",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "nodemon": "^2.0.4"
  },
  "description": "",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AshikRoyce/zen-automation.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Ashik <ashikroyce@gmail.com>",
  "license": "MIT",
  "bugs": {
